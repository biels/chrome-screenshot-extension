    "@types/chrome": "0.0.188",
    "typescript": "^4.4.3",
    "copy-webpack-plugin": "^11.0.0",
    "glob": "^8.0.3",
    "ts-loader": "^9.3.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "rimraf": "^3.0.2",
    "cross-env": "^7.0.3"
  }
